{
  "presets": [
    ["env", {
      "modules": false,
      "targets": { "browsers": ["ie >= 10", "> 5%"] }
    }]
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-runtime",
    "transform-react-jsx",
    [ "react-css-modules", { "generateScopedName": "[local]___[hash:base64:5]" }]
  ]
}